{
  "name": "demos-marketing-intelligence",
  "version": "1.0.0",
  "description": "AI-powered marketing automation for Demos Network - monitors crypto Twitter, generates contextual content, integrates with Typefully",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pipeline": "tsx src/workflows/content-pipeline.ts",
    "dashboard": "cd dashboard && npm run dev",
    "test": "jest",
    "test:brand": "tsx scripts/test-brand-learning.ts",
    "learn:brand": "tsx src/scripts/learn-brand-voice.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "monitor": "tsx src/cli/monitor.ts",
    "generate": "tsx src/cli/generate.ts"
  },
  "keywords": [
    "demos",
    "marketing",
    "automation",
    "crypto",
    "twitter",
    "ai",
    "typefully"
  ],
  "author": "Demos Network",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google-cloud/aiplatform": "^6.1.0",
    "@kynesyslabs/demosdk": "latest",
    "@linear/sdk": "^24.0.0",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.5",
    "better-sqlite3": "^12.6.2",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "openai": "^4.72.0",
    "postgres": "^3.4.3",
    "puppeteer": "^22.0.0",
    "sharp": "^0.33.2",
    "twitter-api-v2": "^1.15.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
